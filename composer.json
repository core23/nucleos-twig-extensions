{
  "name": "core23/twig-extensions",
  "type": "library",
  "description": "Useful extensions for twig.",
  "keywords": [
    "symfony",
    "bundle",
    "twig",
    "extensions",
    "bridge"
  ],
  "homepage": "https://core23.de",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Gripp",
      "email": "mail@core23.de"
    }
  ],
  "require": {
    "php": "^7.1",
    "sonata-project/datagrid-bundle": "^2.2",
    "sonata-project/intl-bundle": "^2.0",
    "twig/twig": "^2.0"
  },
  "require-dev": {
    "matthiasnoback/symfony-dependency-injection-test": "^2.3",
    "symfony/dependency-injection": "^3.4 || ^4.0",
    "symfony/expression-language": "^3.4 || ^4.0",
    "symfony/framework-bundle": "^3.4 || ^4.0",
    "symfony/phpunit-bridge": "^4.0",
    "symfony/routing": "^3.4 || ^4.0",
    "symfony/translation": "^3.4 || ^4.0"
  },
  "autoload": {
    "psr-4": {
      "Core23\\TwigExtensions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Core23\\TwigExtensions\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
